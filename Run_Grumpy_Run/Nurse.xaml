<UserControl x:Class="Run_Grumpy_Run.Nurse"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    d:DesignHeight="300" d:DesignWidth="400" Width="32" Height="32">

    <Canvas x:Name="LayoutRoot" Width="32" Height="32">
        <Canvas.Background>
            <ImageBrush ImageSource="nurse.png" Stretch="Uniform"/>
        </Canvas.Background>
    </Canvas>
</UserControl>
