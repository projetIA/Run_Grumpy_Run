<UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	mc:Ignorable="d"
	x:Class="Run_Grumpy_Run.player"
	d:DesignWidth="640" d:DesignHeight="480" Width="32" Height="32" Loaded="UserControl_Loaded">

	<Canvas x:Name="LayoutRoot">
		<Rectangle x:Name="rect" Height="32" Stroke="Black" Width="32" StrokeThickness="0">
			<Rectangle.Fill>
				<ImageBrush Stretch="Fill" ImageSource="grumpy.png"/>
			</Rectangle.Fill>
		</Rectangle>
	</Canvas>
</UserControl>